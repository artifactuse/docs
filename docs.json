          },
          {
            "group": "Frameworks",
            "pages": [
              "frameworks/react",
              "frameworks/vue",
              "frameworks/svelte",
              "frameworks/vanilla"
            ]
          },
          {
            "group": "Components",
            "pages": [
              "components/agent-message",
              "components/panel",
              "components/panel-toggle"
            ]
          },
          {
            "group": "Artifacts",
            "pages": [
              "artifacts/canvas",
              "artifacts/video",
              "artifacts/code",
              "artifacts/forms",
              "artifacts/social"
            ]
          },
          {
            "group": "AI Prompts",
            "pages": [
              "prompts/overview"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "theming",
              "cdn"
            ]
          }
        ]
      },
      {
        "tab": "Changelog",
        "groups": [
          {
            "group": "Updates",
            "pages": [
              "changelog"
            ]
          }
        ]
      }
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/artifactusehq",
      "github": "https://github.com/artifactuse"
    }
  }
}