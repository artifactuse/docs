{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "styles": [
    "/styles.css"
  ],
  "name": "Artifactuse",
  "colors": {
    "primary": "#6366f1",
    "light": "#f5f3ed",
    "dark": "#6366f1"
  },
  "favicon": "/favicon.png",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "icons": {
    "library": "lucide"
  },
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/artifactuse"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Follow us on X",
      "href": "https://x.com/artifactusehq"
    }
  },
  "navigation": {
    "interaction": {
      "drilldown": true
    },
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "getting-started/introduction",
              "getting-started/quickstart",
              "getting-started/examples"
